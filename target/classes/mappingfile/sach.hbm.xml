<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.Sach" table="sach">
	
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="ten" type="string" column="ten" />
		<property name="maSach" type="string" column="ma_sach" />
		<property name="tacGia" type="string" column="tac_gia" />
		<property name="nhaXuatBan" type="string" column="nha_xuat_ban" />
		<property name="gia" type="string" column="gia" />
		<property name="chuDe" type="string" column="chu_de" />

	</class>
</hibernate-mapping>